{
	"description": "JSON schema for S3 File metadata",
	"type": "interface",
	"implements": [
		{
			"$ref": "org.sagebionetworks.repo.model.file.FileMetadata"
		}
	],
	"properties": {
		"bucketName": {
			"type": "string",
			"description": "The name of the S3 bucket for this file"
		},
		"key": {
			"type": "string",
			"description": "The S3 key for this object"
		},
		"contentType": {
			"description": "Must be: http://en.wikipedia.org/wiki/Internet_media_type",
			"type": "string",
			"required": false
		},
		"contentSize": {
			"description": "The size in bytes.",
			"type": "integer",
			"required": false
		},
		"contentMd5": {
			"description": "MD5 on the content.",
			"type": "string",
			"required": false
		}
	}
}