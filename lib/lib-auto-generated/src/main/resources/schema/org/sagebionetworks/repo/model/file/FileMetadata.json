{
	"description": "JSON schema for file metadata.",
	"type": "interface",
	"properties": {
		"id": {
			"description": "The id of the file.",
			"type": "integer",
			"required": true
		},
		"createdBy": {
			"description": "The user that created this file.",
			"type": "integer"
		},
		"createdOn": {
			"description": "The data when this was created",
			"type": "string",
			"format":"date-time",
		},
        "concreteType":{
            "type":"string"
        }
	}
}